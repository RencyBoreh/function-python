    "#functions are block of reusable coode \n",
    "#return is a statement used to end a function and send results back to the caller\n",
    "def full_name(first,last):\n",
    "    first=first.capitalize()\n",
    "    last=last.capitalize()\n",
    "    return first+\"\"+last\n",
    "fullName=full_name(\"rency\",\"jeptanui\")\n",
    "print(fullName)"
